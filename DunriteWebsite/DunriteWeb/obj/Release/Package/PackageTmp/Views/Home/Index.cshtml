<span class="anchor" id="contact."></span>
<section id="contact">
    <div class="container">
        <div class="row">

            <div class="sec-title text-center wow animated fadeInDown" style="margin-bottom: 20px;">
                <h2 class="text-white">Contact Us</h2>
                <p class="text-white">Fill in the fields below and we'll back to you</p>
            </div>

            <p class="wow animated fadeInLeft text-white text-center responseText"></p>

            <div class="wow animated fadeInLeft col-md-6 col-md-offset-3">
                <form action="#" method="post" id="frmContact">
                    <div class="input-field" style="margin-bottom: 25px;">
                        <input type="text" id="subject" name="subject" class="form-control" placeholder="Name" required>
                    </div>
                    <div class="input-field" style="margin-bottom: 25px;">
                        <input type="email" id="email" name="email" class="form-control" placeholder="Email" required>
                    </div>
                    <div class="input-field" style="margin-bottom: 25px;">
                        <textarea name="message" id="message" class="form-control" placeholder="Message" style="height: 230px;"></textarea>
                    </div>
                    <button type="submit" id="submit" class="btn btn-pink btn-border-4x pull-right">Send Info</button>
                </form>
            </div>

        </div>
    </div>
</section>
